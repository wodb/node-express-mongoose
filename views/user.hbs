<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>user</title>
  <link rel="stylesheet" href="/style/bootstrap.css">
</head>
<body>
  <div class="container">
    <table class="table table-bordered table-striped">
      <thead>
        <th>姓名</th>
        <th>用户名</th>
        <th>密码</th>
        <th>性别</th>
        <th>年龄</th>
        <th>电话</th>
        <th>博客</th>
        <th>详细信息</th>
      </thead>
      <tbody>
        {{#each data}}
          <tr>
            <td>{{this.name}}</td>
            <td>{{this.username}}</td>
            <td>{{this.password}}</td>
            <td>{{this.sex}}</td>
            <td>{{this.age}}</td>
            <td>{{this.telphone}}</td>
            <td>{{this.blog}}</td>
            <td>{{this.detail}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</body>
</html>